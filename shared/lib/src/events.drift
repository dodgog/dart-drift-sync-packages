import 'aaausers.drift';

CREATE TABLE events (
    id text NOT NULL,
    type TEXT NOT NULL,
    client_id TEXT NOT NULL REFERENCES clients(id),
    server_time_stamp TEXT, -- TODO: dialect aware date https://drift.simonbinder.eu/sql_api/types/#dialect-awareness
    client_time_stamp TEXT NOT NULL,
    content TEXT -- TODO: add type converter https://drift.simonbinder.eu/type_converters/#type-converters-and-json-serialization
);
